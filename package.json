{
  "packageManager": "pnpm@8.8.0",
  "author": "Zero <gczgroup@qq.com>",
  "scripts": {
    "build": "pnpm -F vite-nest build",
    "build:module": "pnpm -F vite-nest-module build",
    "dev": "pnpm -F base dev",
